{
	"name": "baraja-core/zasilkovna",
	"description": "Zasilkovna SOAP and REST client in PHP",
	"keywords": [
		"Zasilkovna",
		"SOAP",
		"API",
		"REST"
	],
	"homepage": "https://github.com/baraja-core/zasilkovna",
	"license": [
		"LGPL-3.0"
	],
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		},
		{
			"name": "Adam Schubet",
			"homepage": "https://github.com/Salamek/zasilkovna"
		}
	],
	"require": {
		"php": "^8.0",
		"tecnickcom/tcpdf": "^6.3",
		"spatie/array-to-xml": "^2.15"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.74",
		"tracy/tracy": "^2.8",
		"phpstan/phpstan-nette": "^0.12.14",
		"spaze/phpstan-disallowed-calls": "^1.1",
		"roave/security-advisories": "dev-master",
		"jetbrains/phpstorm-attributes": "^1.0"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
